function [r,c] = qwerty_coord(key)
   A={'1','2','3','4','5','6','7','8','9','0';'q','w','e','r','t','y','u','i','o','p';
       'a','s','d','f','g','h','j','k','l',':';'z','x','c','v','b','n','m','<','>','?'};
   for i=1:4
       for j=1:10
           if A{i,j}==key;
               r=i;
               c=j;
               break;
           end
       end
   end
end
